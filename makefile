clean: 
	find . -name \*.fdb_latexmk -type f -delete && find . -name \*.idx -type f -delete && find . -name \*.ilg -type f -delete && find . -name \*.ind -type f -delete && find . -name \*.log -type f -delete && find . -name \*.out -type f -delete && find . -name \*.synctex.gz -type f -delete && find . -name \*.toc -type f -delete && find . -name \*.xdv -type f -delete


